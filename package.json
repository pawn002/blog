{
	"name": "pawn002-blog",
	"version": "0.0.0",
	"description": "pawn002 Blog",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"copy-docs": "del /Q docs\\* && rmdir /S /Q docs && mkdir docs && xcopy _site\\* docs\\ /E /H /C /I",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/pawn002-blog/ && npm run copy-docs",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/pawn002-blog/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pawn002/blog"
	},
	"author": {
		"name": "John Rioflorido",
		"email": "john.rioflorido@outlook.com",
		"url": "https://github.com/pawn002"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"bugs": {
		"url": "https://github.com/pawn002/blog-public/issues"
	},
	"homepage": "https://github.com/pawn002/blog-public",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.5"
	}
}
